digraph SampleTactic {
	failed [label="behavior::failed" shape=ellipse]
	completed [label="behavior::completed" shape=ellipse]
	start [label="behavior::start" shape=diamond]
	cancelled [label="behavior::cancelled" shape=ellipse]
	subgraph cluster_0 {
		graph [label="behavior::running" style=dotted]
		running [label="sample_tactic::running" shape=ellipse]
		preparing [label="sample_tactic::preparing" shape=ellipse]
	}
	start -> preparing [label=immediately decorate=True]
	preparing -> running [label=ready decorate=True]
}
